{
  "name": "featherlog",
  "version": "2.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "pnpm build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "logging",
    "nodejs",
    "typescript",
    "sdk",
    "error-logging",
    "logger",
    "remote-logging",
    "centralized-logging"
  ],
  "author": "Martijn Dorsman",
  "license": "ISC",
  "description": "A simple logging SDK for Node.js that sends logs to your Featherlog server",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martijnd/featherlog.git",
    "directory": "packages/sdk"
  },
  "bugs": {
    "url": "https://github.com/martijnd/featherlog/issues"
  },
  "homepage": "https://github.com/martijnd/featherlog/tree/main/packages/sdk#readme",
  "readme": "https://github.com/martijnd/featherlog/blob/main/packages/sdk/README.md",
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/node": "^20.0.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  }
}
